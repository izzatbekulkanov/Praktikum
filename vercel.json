{
  "builds": [{
    "src": "setting/wsgi.py",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "400mb", "runtime": "python3.10.12"}
  }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "setting/wsgi.py"
    }
  ]
}